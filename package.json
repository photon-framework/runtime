{
  "name": "photon-re",
  "displayName": "photon runtime",
  "version": "1.0.0",
  "type": "module",
  "description": "Simple and high performance web frontend framework",
  "main": "index.js",
  "scripts": {
    "build": "yarn run tsc -p ./tsconfig.json",
    "copy": "yarn build && rm -rf ../project-creator/node_modules/photon-re/* && cp -r ./src ../project-creator/node_modules/photon-re/src && cp -r ./node_modules/xxhash-wasm ../project-creator/node_modules/ && cp ./* ../project-creator/node_modules/photon-re/",
    "deploy": "yarn run build && yarn publish",
    "docs": "rm -rf ./docs/* && rm -rf ../photon.wiki/*.md && node docs.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/photon-framework/runtime.git"
  },
  "keywords": [
    "framework",
    "web"
  ],
  "author": "Frank Mayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/photon-framework/runtime/issues"
  },
  "homepage": "https://github.com/photon-framework",
  "devDependencies": {
    "jsdoc-to-markdown": "^7.1.1",
    "typedoc": "^0.23.5",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@frank-mayer/magic": "^4.0.0",
    "tslib": "^2.4.0",
    "xxhash-wasm": "^1.0.1"
  }
}
